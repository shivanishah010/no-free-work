{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/subscription-tracker",
      "destination": "https://freelance-subscriptions-tracker.vercel.app/"
    },
    {
      "source": "/subscription-tracker/(.*)",
      "destination": "https://freelance-subscriptions-tracker.vercel.app/$1"
    },
    {
      "source": "/assets/(.*)",
      "destination": "https://freelance-subscriptions-tracker.vercel.app/assets/$1"
    }
  ]
}
